{
  "name": "My Extension Page",
  "description": "Base Level Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "page_action": {
    "default_popup": "page_popup.html",
    "default_icon": "demo1-logo.png"
  },
  "commands": {
    "_execute_page_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens page_popup.html"
    }
  },
  "background": {
    "scripts": ["bg.js"]
  }
}